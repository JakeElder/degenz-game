{
  "name": "data",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "dev": "node scripts/dev",
    "typecheck": "tsc --noEmit",
    "migration:show": "typeorm -d src/data-source migration:show",
    "migration:run": "typeorm -d src/data-source migration:run",
    "migration:generate": "typeorm -d src/data-source migration:generate",
    "migration:create": "typeorm -d src/data-source migration:create",
    "schema:log": "typeorm -d src/data-source schema:log",
    "schema:sync": "typeorm -d src/data-source schema:sync",
    "orm": "typeorm -d src/data-source"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/pg-connection-string": "^2.0.0",
    "lib": "*",
    "tsconfig": "*",
    "typescript": "^4.6.1-rc"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@types/find-config": "^1.0.1",
    "class-transformer": "^0.5.1",
    "discord-api-types": "^0.31.1",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "find-config": "^1.0.0",
    "pg-connection-string": "^2.5.0",
    "random-item": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "type-fest": "^2.12.2",
    "typeorm": "^0.3.5",
    "typeorm-naming-strategies": "^4.1.0"
  }
}
