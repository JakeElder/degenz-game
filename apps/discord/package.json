{
  "name": "discord",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf dist && concurrently --raw \"yarn watch\" \"wait-on dist/main.js && yarn start:dev\"",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/main",
    "start:dev": "nodemon --exec \"node -r source-map-support/register dist/main\"",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "watch": [
      "dist",
      "../../packages/types/dist",
      "../../packages/manifest/dist",
      "../../packages/lib/dist",
      "../../packages/config/dist"
    ]
  },
  "dependencies": {
    "@types/source-map-support": "^0.5.4",
    "discord.js": "^13.6.0",
    "lib": "*",
    "manifest": "*",
    "node-cleanup": "^2.1.2",
    "pretty-error": "^4.0.0",
    "types": "*"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/node-cleanup": "^2.1.2",
    "@types/react": "17.0.37",
    "concurrently": "^7.0.0",
    "config": "*",
    "eslint": "7.32.0",
    "nodemon": "^2.0.15",
    "source-map-support": "^0.5.21",
    "tsconfig": "*",
    "typescript": "^4.5.3",
    "wait-on": "^6.0.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.14.0"
  }
}
