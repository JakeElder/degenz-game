{
  "name": "cli",
  "version": "0.0.0",
  "description": "Degenz Game Admin CLI",
  "bin": {
    "degenz": "./bin/run",
    "dg": "./bin/run"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/node": "^17.0.17",
    "@types/prettyjson": "^0.0.30",
    "axios": "^0.25.0",
    "discord-api-types": "^0.27.0",
    "prettyjson": "^1.2.5"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "degenz",
    "dirname": "degenz",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "server": {
        "description": "Operations relating to the server"
      }
    }
  },
  "engines": {
    "node": "^16.14.0"
  },
  "bugs": "https://github.com/JakeElder/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
