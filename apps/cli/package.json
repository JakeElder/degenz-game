{
  "name": "cli",
  "version": "0.0.0",
  "description": "Degenz CLI",
  "author": "Jake Elder @JakeElder",
  "bin": {
    "degenz": "./bin/dev"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "typecheck": "yarn tsc --noEmit",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "license": "UNLICENSED",
  "main": "src/index.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/cli-progress": "^3.9.2",
    "@types/prompts": "^2.0.14",
    "axios": "^0.27.2",
    "chalk": "^4.0.0",
    "cli-progress": "^3.10.0",
    "color": "^4.2.3",
    "convert-array-to-csv": "^2.0.0",
    "data": "*",
    "deepmerge": "^4.2.2",
    "discord.js": "^13.6.0",
    "humanize-duration": "^3.27.2",
    "manifest": "*",
    "prompts": "^2.4.2",
    "random-item": "^3.0.1",
    "recursive-copy": "^2.0.14",
    "short-uuid": "^4.2.0",
    "ts-dedent": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/color": "^3.0.3",
    "@types/deepmerge": "^2.2.0",
    "@types/humanize-duration": "^3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.3"
  },
  "oclif": {
    "bin": "degenz",
    "dirname": "degenz",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topics": {
      "achievements": {
        "description": "Manage Achievements"
      },
      "channels": {
        "description": "Manage Channels"
      },
      "districts": {
        "description": "Manage Districts"
      },
      "dormitories": {
        "description": "Manage Dormitories"
      },
      "emojis": {
        "description": "Manage Emojis"
      },
      "mart-items": {
        "description": "Manage Mart Items"
      },
      "npcs": {
        "description": "Manage NPCs"
      },
      "persistent-messages": {
        "description": "Manage Persistent Messages"
      },
      "roles": {
        "description": "Manage Roles"
      }
    }
  },
  "engines": {
    "node": "^16.14.0"
  },
  "types": "dist/index.d.ts"
}
