{
  "name": "cli",
  "version": "0.0.0",
  "description": "Degenz Game Admin CLI",
  "bin": {
    "degenz": "./bin/run",
    "dg": "./bin/run"
  },
  "scripts": {
    "build": "shx rm -rf dist && yarn tsc -b",
    "dev": "ts-node scripts/dev",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest",
    "version": "oclif readme && git add README.md"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/prettyjson": "^0.0.30",
    "axios": "^0.25.0",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "config": "*",
    "delay": "^5.0.0",
    "discord-api-types": "^0.27.0",
    "discord.js": "^13.6.0",
    "lib": "*",
    "listr-verbose-renderer": "^0.6.0",
    "manifest": "*",
    "prettyjson": "^1.2.5"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.9.4",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "oclif": "^2",
    "recursive-copy": "^2.0.14",
    "shx": "^0.3.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsc-watch": "^4.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.1-rc",
    "wait-on": "^6.0.1"
  },
  "oclif": {
    "bin": "degenz",
    "dirname": "degenz",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "server": {
        "description": "Operations relating to the server"
      }
    }
  },
  "engines": {
    "node": "^16.14.0"
  },
  "bugs": "https://github.com/JakeElder/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
